    "test:spec": "node --test",
    "test:typescript": "npm run build && ts-mocha test/**/*.spec.ts",
    "test:lint": "eslint src --fix"
  },
  "devDependencies": {
    "@xmldom/xmldom": "^0.8.0",
    "esbuild": "0.24.0",
    "eslint": "^9.13.0",
    "resolve": "^1.22.8",
    "sanitize-filename": "^1.6.3",
    "typescript": "^4.3.2",
    "validate-with-xmllint": "^1.2.0",
    "xpath.js": "^1.1.0"
  },
  "dependencies": {
    "chord-symbol": "^3.0.0",
    "fast-diff": "^1.2.0",
    "jstoxml": "^2.0.6",
    "promise": "^8.1.0"
  }
